{
  "name": "middle.chat.praktikum",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.17.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run prebuild && vite build",
    "prebuild": "tsc --noEmit",
    "start": "vite build && vite preview --port 3000 --host",
    "lint": "eslint . --ignore-pattern \"dist/**\" --fix -c eslint.config.js && stylelint \"src/**/*.scss\" --fix && tslint -c tslint.json 'src/**/*.ts' --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/lodash-es": "^4.17.12",
    "eslint": "^8.57.1",
    "globals": "^15.13.0",
    "sass-embedded": "^1.81.0",
    "stylelint": "^16.11.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.0",
    "vite": "^4.5.5"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "sass": "^1.81.0",
    "scss": "^0.2.4",
    "tslint": "^6.1.3",
    "vite-plugin-handlebars": "^2.0.0"
  }
}
